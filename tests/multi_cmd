echo hello world || ls -l
echo this is a multi command test; ls
ps; history
mkdir test && echo made test directory
ls -l; touch test_file; echo created test file && ls
mkdir test || echo test directory already exists
rm test_file && echo test file removed; ls -l
cat fail || echo invalid usage of cat
ls && ls -l; ls -a || ls
echo && mkdir || touch || ls
echo removing test directories; rm -rf test
ls -al; ls || history
history; ps; echo test complete
exit
